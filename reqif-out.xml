      <SPEC-TYPES>
        <SPEC-OBJECT-TYPE xmlns:reqif.xsd="http://www.omg.org/spec/ReqIF/20110401/reqif.xsd" reqif.xsd:DESC="A functional requirement describes a function of the system, i.e. what the system must do resp. a service the system provides to its users." reqif.xsd:IDENTIFIER="FUNC-REQ" reqif.xsd:LAST-CHANGE="2011-06-13T10:24:18.000+01:00" reqif.xsd:LONG-NAME="Functional Requirement">
          <ALTERNATIVE-ID></ALTERNATIVE-ID>
          <SPEC-ATTRIBUTES>
            <ATTRIBUTE-DEFINITION-ENUMERATION reqif.xsd:DESC="The status of the requirement." reqif.xsd:IDENTIFIER="FUNC-REQ-STATUS" reqif.xsd:LAST-CHANGE="2011-06-13T10:24:18.000+01:00">
