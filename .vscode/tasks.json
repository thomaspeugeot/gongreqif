{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"group": "build",
			"label": "gowsdl people.wsdl",
			"command": "gowsdl -d go -p schema -o schema.go reqif.wsdl"
		},
		{
			"label": "02 - run gongreqif",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongreqif"
			},
			"command": "go",
			"args": [
				"run",
				"main.go",
				"readfile.go"
			],
			"group": "build"
		},
		{
			"label": "03 - ng build",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongreqif; ng build",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "04 - ng serve",
			"type": "shell",
			"command": "cd ng-github.com-fullstack-lang-gongreqif; ng serve",
			"group": "build",
			"problemMatcher": []
		},
		{
			"label": "01 - gongc gongreqif",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": []
		},
		{
			"label": "01 - gongc gongreqif -skipNg",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipNg"
			]
		},
		{
			"label": "01 - gongc gongreqif -skipGoModCommands",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipGoModCommands"
			]
		},
		{
			"label": "01 - gongc gongreqif -skipNg -skipGoModCommands -compileForDebug go/models",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipGoModCommands",
				"-compileForDebug",
				"-skipNg"
			]
		},
		{
			"label": "01 - gongc gongreqif -skipNg -skipGoModCommands",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/models"
			},
			"command": "gongc",
			"group": "build",
			"args": [
				"-skipNg",
				" -skipGoModCommands"
			]
		},
		{
			"label": "00 - go mod vendor",
			"type": "shell",
			"group": "build",
			"command": "go mod vendor"
		},
		{
			"label": "00 - update dependencies",
			"type": "shell",
			"command": "go",
			"args": [
				"get",
				"-d",
				"github.com/fullstack-lang/gong@HEAD",
				"github.com/fullstack-lang/gongdoc@HEAD",
				"github.com/fullstack-lang/gongsvg@HEAD",
			],
			"group": "build"
		},
		{
			"label": "00 - go get -d github.com/fullstack-lang/gong@HEAD",
			"type": "shell",
			"command": "go",
			"args": [
				"get",
				"-d",
				"github.com/fullstack-lang/gong@HEAD",
			],
			"group": "build"
		},
		{
			"label": "00 - go get -d github.com/fullstack-lang/gongdoc@HEAD",
			"type": "shell",
			"command": "go",
			"args": [
				"get",
				"-d",
				"github.com/fullstack-lang/gongdoc@HEAD",
			],
			"group": "build"
		},
		{
			"label": "00 - go mod tidy",
			"type": "shell",
			"command": "go",
			"args": [
				"mod",
				"tidy"
			],
			"group": "build"
		},
		{
			"label": "00 - go build",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/go/cmd/gongreqif"
			},
			"command": "go",
			"args": [
				"build"
			],
			"group": "build"
		},
	]
}