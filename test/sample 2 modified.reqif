                </SPEC-OBJECT>
                <SPEC-OBJECT reqif.xsd:DESC="" reqif.xsd:IDENTIFIER="R002"
                    reqif.xsd:LAST-CHANGE="2011-06-13T10:24:18+01:00"
                    reqif.xsd:LONG-NAME="Spotting fire">
                    <TYPE>
                        <SPEC-OBJECT-TYPE-REF>FUNC-REQ</SPEC-OBJECT-TYPE-REF>
                    </TYPE>
                    <VALUES>
                        <ATTRIBUTE-VALUE-STRING
                            reqif.xsd:THE-VALUE="The system shall alert the control center if it spots a fire in a forest.">
